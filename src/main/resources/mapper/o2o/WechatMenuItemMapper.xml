<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="erp.chain.mapper.o2o.WechatMenuItemMapper">
    <select id="getMenuItemById" resultType="erp.chain.domain.o2o.WxMenuItem">
        SELECT * FROM wx_menu_item WHERE id=#{itemId} AND is_deleted=0
